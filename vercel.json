{
    "version": 2,
    "builds": [
      { "src": "dist/authentication/**", "use": "@vercel/static" },
      { "src": "dist/admin/**", "use": "@vercel/static" },
      { "src": "dist/client/**", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/authentication/(.*)", "dest": "/authentication/$1" },
      { "src": "/admin/(.*)", "dest": "/admin/$1" },
      { "src": "/(.*)", "dest": "/client/$1" }
    ]
  }
  